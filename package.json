{
  "name": "tellevo-frontend-login",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build --mode production",
    "build:clean": "pnpm run clean && pnpm run build",
    "preview": "vite preview",
    "build:debug": "NODE_ENV=production vite build --mode production --debug",
    "clean": "rm -rf dist node_modules/.vite pnpm-lock.yaml.bak",
    "clean:all": "pnpm run clean && rm -f package-lock.json",
    "postinstall": "echo 'Installation complete'",
    "typecheck": "vue-tsc --build --force"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "caniuse-lite": "1.0.30001743",
    "chart.js": "^4.4.8",
    "daisyui": "^5.0.16",
    "express": "^5.1.0",
    "lucide-vue-next": "^0.487.0",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0",
    "vue-tsc": "^2.1.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not IE 11",
    "maintained node versions"
  ]
}
